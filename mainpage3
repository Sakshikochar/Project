  <script>// Get all the like button and counter elements from the DOM
const likeBtns = document.querySelectorAll(".likeBtn");
const counters = document.querySelectorAll(".counter");

// Initialize an array of like counts
const likeCounts = Array.from({ length: likeBtns.length }, () => 0);

// Add an event listener to each button
likeBtns.forEach((btn, index) => {
  btn.addEventListener("click", () => {
    // Increment the like count for this button
    likeCounts[index]++;
    
    // Update the counter text for this button
    counters[index].textContent = `${likeCounts[index]} like${likeCounts[index] !== 1 ? 's' : ''}`;
  });
});
</script>
